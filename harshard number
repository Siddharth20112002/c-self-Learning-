#include<iostream>
using namespace std;
int main()
{
    int n,a,b,c,d,n1;
    cin>>n;
    a=n/1000;
    b=(n%1000)/100;
    c=((n%1000)/100)/10;
    d=((n%1000)/100)%10;
    n1=n/(a+b+c+d);
    if(n1%n==0)
    {
        cout<<"Harshard Number";
    }
    else
    {
        cout<<"Not Harshard Number";
    }
}
    
