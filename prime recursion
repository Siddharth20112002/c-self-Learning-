#include<iostream>
using namespace std;
int prime(int a)
{
    int c=0;
  
    for(int i=1;i<=a;i++)
    {
        if(a%i==0)
        {
        c++;
        }
    }
    return c;
    
}
int main()
{
    int a;
    cin>>a;
    if(prime(a)<=2)
    { 
        cout<<a<<" "<<"is a prime number";
    }
    else
    {
        cout<<a<<" "<<"is not a prime number";
    }
    
}
