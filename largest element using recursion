#include<iostream>
using namespace std;
int max(int n)
{
    int a[n],i;
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    
    }
    for(i=0;i<n;i++)
    {
       if (a[0]<a[i])
       {
           a[0]=a[i];
       }
    } cout<<"Largest element of on array is "<<a[0];
    return 1;
}
int main()
{
    int n;
    cin>>n;
    max(n);
}
